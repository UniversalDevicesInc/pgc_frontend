{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --host 0.0.0.0 --port 8888 --disable-host-check",
    "start:devs": "ng serve --port 8080 --host polyglot --ssl true --ssl-key ~/.polyglot/ssl/polyglot_private.key --ssl-cert ~/.polyglot/ssl/polyglot.crt --disable-host-check",
    "build:prod": "ng build --prod --output-path=dist/frontend/prod; aws s3 sync --profile pgc-prod --delete --include=* dist/frontend/prod s3://polyglot-prod.isy.io/ --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
    "build:test": "ng build --configuration=test --output-path=dist/frontend/test; aws s3 sync --profile udi-pgc --delete --include=* dist/frontend/test s3://pgtest.isy.io/ --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
    "build:dev": "ng build --output-path=dist/frontend/dev; aws s3 sync --profile c42admin --delete --include=* dist/frontend/dev s3://pgdev.isy.io/ --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.15",
    "@angular/common": "7.2.15",
    "@angular/compiler": "7.2.15",
    "@angular/core": "7.2.15",
    "@angular/forms": "7.2.15",
    "@angular/http": "7.2.15",
    "@angular/platform-browser": "7.2.15",
    "@angular/platform-browser-dynamic": "7.2.15",
    "@angular/platform-server": "7.2.15",
    "@angular/router": "7.2.15",
    "@ng-bootstrap/ng-bootstrap": "4.1.3",
    "core-js": "3.1.2",
    "mqtt": "2.18.8",
    "ngx-spinner": "7.1.4",
    "ngx-toastr": "10.0.4",
    "npm": "^6.9.0",
    "pako": "1.0.10",
    "rxjs": "6.5.2",
    "rxjs-compat": "6.5.2",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.9",
    "@angular/cli": "7.3.9",
    "@angular/compiler-cli": "7.2.15",
    "@angular/language-service": "7.2.15",
    "@types/jasmine": "3.3.12",
    "@types/jasminewd2": "2.0.6",
    "@types/mqtt": "2.5.0",
    "@types/node": "12.0.2",
    "codelyzer": "5.0.1",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "protractor": "5.4.2",
    "rxjs-tslint": "0.1.7",
    "ts-node": "~8.1.0",
    "tslint": "5.16.0",
    "typescript": "3.2.4",
    "webpack": "4.32.2"
  }
}
